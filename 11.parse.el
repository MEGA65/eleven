CLARE DESTØLINE$(1000)              ' POST PROCESSED LINES#DECLARE ELEMENTØCNT(4)                ' ELEMENT COUNT PER TYPE#DECLARE VARØTABLE$(4,200)             ' VARIABLE TABLE PER TYPE#DECLARE DEFINEØVAL$(200)              ' DEFINE VALUES TABLE#DECLARE LABELØNAME$(200)              ' LABEL TABLE#DECLARE LABELØLINENO(200)#DECLARE LABELØCNT=0#DECLARE ARGS$(32)                     ' ARGUMENT LIST#DECLARE STRUCTØNAME$(30)#DECLARE STRUCTØFIELDS$(30)#DECLARE STRUCTØVARS$(30)              ' STRUCT VARS (EACH ENTRY HAS STRING OF VARS)#DECLARE STRUCTØCNT=0                  ' INDEX TO NEXT FREE STRUCT DEFINITION#DECLARE WHITESPACE$, SRCØLINENO, CURØSRCØLINENO, CB#DECLARE CURØATTICØADDR, TOTALØLINES, S$, DELETEØLINEØFLAG#DECLARE VERBOSE, DESTØLINENO, INSIDEØIFDEF, Z$, CURØDESTØLINE$#DECLARE CURØDESTØLINENO, S1, S2, CURØTOK$, R, F$, DEFINEØFLAG, DELIM$#DECLARE IGNOREØBRACKETS, ARGØCNT, PARSERØERROR$, VARØNAME$#DECLARE DIMENSION$, VALUE$, BKTØOPENØIDX, BKTØCLOSEØIDX, EQUALSØIDX#DECLARE TR$, HX$, BI$, DONTØMARKØLABEL, TY, ID, GENØVARNAME$, OF$#DECLARE ARGSØLIST$, ARGSØLISTØLEN, IGNOREØDELIMØFLAG, ARGSØIDX, CURØCH$#DECLARE QUOTEØFLAG, DIDØREPLACEØFLAG#DECLARE A$, EXPECTINGØLABEL, DEFAULTØDELIM$, RVØIDX#DECLARE T, LC$, FOUNDØSTRUCTØIDX, BR, B, BC$, VL#DECLARE N1, N2, BA, A, AD, TF$, FOUNDØIDX, K, SF$, SF#DECLARE CURØLINEØLEN, SRCØLINEØPTR, SRCØLINEBUFFØPTR, CUTØTAILØIDX#DECLARE CURØLINEØLENØMINUSØONE, CURØLINEBUFFØADDR, CHR#DECLARE CO$, RIDX, SK$, BL, SZ, SR, SM, ZZ$#DECLARE CONTØNEXTØLINEØFLAG, TOKØNAME$, CLEANØVARNAME$#DECLARE SHITTYØSYNTAXØFLAG, ZZ'----.MAIN'----  PARSERØFILE$ = "11.PARSE"  MK$ = "@~"  ' @ AND PI  DBLØQUOTE$ = CHR$(34)  DBLØQUOTEØCHAR = 34  SNGLØQUOTEØCHAR = 39  KEY ON  PRINT "≈Ã≈÷≈Œ PREPROCESSOR V0.6.0í"  PRINT  KEY 7, "SCRATCH" + DBLØQUOTE$ + PARSERØFILE$ + DBLØQUOTE$ + ":DSAVE" + DBLØQUOTE$ + PARSERØFILE$ + DBLØQUOTE$ + ":DVERIFY" + DBLØQUOTE$+PARSERØFILE$  T$="                                                                               "  BLANKØLINE$ = T$ + T$ + T$  T$ = ""TOKENS$(0)=" PRINT INPUT IF THEN ELSE DO LOOP WHILE UNTIL GOSUB GOTO OPEN CLOSE DOPEN DCLOSE FOR NEXT GETKEY HEX$ DIM PEEK POKE WAIT DEC CHR$ ASC SGN SQR STR$"  TOKENS$(0) = TOKENS$(0) + " GRAPHIC CLR SCREEN DEF BEGIN BEND LEN MID$ RIGHT$ LEFT$ INSTR FOR NEXT STEP TRAP BORDER AND FOREGROUND "  TOKENS$(1) = " BACKGROUND SET ABS SIN COS TAN LOG FRE CURSOR PIXEL WINDOW RWINDOW LINE BOX CIRCLE ELLIPSE PALETTE RESTORE DATA ERR$ ER EL CURSOR ON OFF"  TOKENS$(1) = TOKENS$(1) + " VAL SCRATCH RETURN RND STOP BANK TI DO OR ST IF EL ER ON TO PEN GET END INT NOT DS DS$ RUN USING DOT "  TOKENS$(2) = " APPEND ATN AUTO BACKUP BLOAD BOOT BSAVE BUMP BVERIFY CATALOG CHANGE CHAR CMD COLLISION COLOR CONCAT CONT COPY WPOKE WPEEK SETBIT CLRBIT "  TOKENS$(3) = " DCLEAR DEFFN DELETE FN DIR DISK DLOAD DMA DMODE DPAT DSAVE DVERIFY EDMA ENVELOPE ERASE EXIT EXP FAST FILTER FIND GO GO64 HEADER HELP HIGHLIGHT "  TOKENS$(4) = " HASBIT JOY LET LIST LOAD LOG10 LOG2 LOCATE LPEN MOD MONITOR MOUSE MOVSPR NEW PAINT PLAY POINTER POLYGON POS POT PUDEF "  TOKENS$(5) = " RCOLOR RDISK RDOT READ RECORD REM RENAME RESUME RGRAPHIC RMOUSE RPLAY RPT$ RREG RSPCOLOR RSPPOS RSPRITE SAVE SCNCLR SLEEP SLOW SOUND SPC SPRCOLOR "  TOKENS$(6) = " SPRITE SPRSAV SYS TAB TEMPO TROFF TRON TYPE USR VERIFY VOL XOR KEY VSYNC RCURSOR T@& C@& RGRAPHIC FREAD POINTER CHARDEF "  DUMBØCMDS$ = " BLOAD BSAVE DLOAD TO SAVE DIR COLLECT DOPEN DCLOSE BACKUP FREAD GET "  GOSUB GETØFILENAME  BANK 128  TYPEØIDENT$(‘Ÿ–Ø“≈¡Ã)=""  TYPEØIDENT$(‘Ÿ–Ø…Œ‘)="%"  TYPEØIDENT$(‘Ÿ–Ø”‘“)="$"  TYPEØIDENT$(‘Ÿ–Ø¬Ÿ‘≈)="&"  BINØCONV(0) = 1  FOR I = 1 TO 16    BINØCONV(I) = BINØCONV(I - 1) + BINØCONV(I - 1)  NEXT I' ------------------------- PASS 1 ------------------------------------'------.PASSØ1'------  NEXTØLINEØFLAG = 0  WHITESPACE$ = CHR$(32) + CHR$(160) + "	"  ' CLEANUP TEMPORARY FILES  SRCØLINENO = 0  PRINT "PASS 1 ";  CURØSRCØLINENO=0  CLR TI  ' KEEP START TIME FOR TIMING  CB = $8030000   ' ELEVEN SOURCE TO BE COMPILED IS LOCATED HERE IN ATTIC RAM  CURØATTICØADDR = CB  TOTALØLINES = WPEEK(CURØATTICØADDR)  CURØATTICØADDR = CURØATTICØADDR + 2  DO WHILE CURØSRCØLINENO <> TOTALØLINES  ' UNTIL TARGET LINES IS REACHED    GOSUB READØNEXTØLINE    GOSUB SINGLEØQUOTEØCOMMENTØTRIM    ' STRIP WHITESPACE FROM END    S$ = CURØSRCØLINE$    GOSUB STRIPØTR$ØFROMØEND    CURØSRCØLINE$ = S$    IF CURØSRCØLINE$ <> "" THEN BEGIN      DELETEØLINEØFLAG = 0      IF VERBOSE THEN PRINT ">> ƒ≈”‘:" DESTØLINENO;", ”“√: "; SRCØLINENO;": "; CURØSRCØLINE$      IF LEFT$(CURØSRCØLINE$, 1) = "." THEN BEGIN        NEXTØLINEØFLAG = 1        GOSUB ADDØTOØLABELØTABLE      BEND      IF LEFT$(CURØSRCØLINE$, 1) = "#" THEN BEGIN        GOSUB PARSEØPREPROCESSORØDIRECTIVE      BEND      IF INSIDEØIFDEF = 1 THEN GOTO PARSERØLOOPØSKIP      IF LEFT$(CURØSRCØLINE$, 4) = "DATA" OR RIGHT$(CURØSRCØLINE$, 5) = "BEGIN" THEN BEGIN        NEXTØLINEØFLAG = 1      BEND      IF DELETEØLINEØFLAG = 0 THEN BEGIN        IF VERBOSE = 0 THEN PRINT ".";        S$ = CURØSRCØLINE$        GOSUB REPLACEØVARSØANDØLABELS        ' GOSUB CHECKØFORØCREATIONØOFØSTRUCTØOBJECT        '   (CAN'T BE DONE HERE, IT IS DONE INSIDE REPLACEØVARSØANDØLABELS: INSTEAD)        GOSUB CHECKØFORØCONTINUEØONTOØNEXTØLINE        ' SAFE ADD CURØDESTØLINE$+S$ TO CURRENT OR NEXT DESTØLINE$(DESTØLINENO)        GOSUB SAFEØADDØTOØCURRENTØORØNEXTØLINE        IF RIGHT$(S$, 4) = "BEND" OR RIGHT$(S$, 6) = "RETURN" _           OR LEFT$(S$, 2) = "IF" THEN BEGIN          NEXTØLINEØFLAG = 1        BEND      BEND  ' ENDIF DELETEØLINEØFLAG = 0    BEND  ' ENDIF CURØSRCØLINE$ <> "".PARSERØLOOPØSKIP    ' INCREASE SOURCE CODE LINE (FOR ERROR MSGS...)    SRCØLINENO = SRCØLINENO + 1    IF VERBOSE THEN BEGIN      PRINT "SRCØLINENO="; SRCØLINENO      GET KEY Z$    BEND  LOOP    IF CURØDESTØLINE$ <> "" THEN BEGIN    DESTØLINE$(DESTØLINENO) = CURØDESTØLINE$    DESTØLINENO = DESTØLINENO + 1  BEND  CLOSE 1  GOSUB SAVEØFILENAME  ' SET OUTPUT FILENAME  SCRATCH "11TEMP"  SCRATCH "11TOKENIZED"' ------------------------- PASS 2 ------------------------------------'------.PASSØ2'------  OPEN 1,1,5,"11TEMP,S,W"  PRINT CHR$(13) "PASS 2 ";  FOR CURØDESTØLINENO = 0 TO DESTØLINENO - 1    S$ = DESTØLINE$(CURØDESTØLINENO)    IF VERBOSE THEN PRINT CURØDESTØLINENO; "û=> " S$ : ELSE PRINT ".";    DO WHILE INSTR(S$, MK$) <> 0       S1 = INSTR(S$, MK$) : S2 = INSTR(S$, MK$, S1 + 2)       IF S2 = 0 THEN BEGIN         EXIT  ' EXIT LOOP FOR CASES WHERE A PI SYMBOL WAS USED IN THE SOURCE       BEND:ELSE BEGIN         CURØTOK$ = MID$(S$, S1 + 2, S2 - S1 - 2)         GOSUB SWAPØOUTØLABELØINØCURØTOK$ØFORØLINENO         S$ = LEFT$(S$, S1 - 1) + CURØTOK$ + MID$(S$, S2 + 2)       BEND    LOOP    IF VERBOSE THEN PRINT "<= "; STR$(CURØDESTØLINENO) + S$    PRINT #1, STR$(CURØDESTØLINENO) + " " + S$  NEXT CURØDESTØLINENO  GOSUB DUMPØVARS  FOR R = 0 TO 10    PRINT #1, STR$(32000 + R)  NEXT R  ' REMOVE 11.TOKENISE.BAS LINES?  F$ = "D√:D”" + DBLØQUOTE$ + "11TOKENIZED" + DBLØQUOTE$  F$ = F$ + ":IFDS<>0THEN?" + DBLØQUOTE$ + "DISC ERROR: "  F$ = F$ + DBLØQUOTE$ + ";DS$:ELSE?" + DBLØQUOTE$  F$ = F$ + ""  F$ = F$ + DBLØQUOTE$ + ":DÃ" + DBLØQUOTE$ + "11.POST"  PRINT #1, F$  CLOSE 1  PRINT""  PRINT "ELAPSED TIME:"; TI; "TICKS"  PRINT"ìEDMA 0,$1FFF,$8022000,$2001:NEW RESTORE"  PRINT"RUN";  ' LOAD '11.TOKENIZE' FROM CACHE  BANK 128  POKE 208,2      ' NO# OF CHARS IN KEYBOARD BUFFER  POKE 688,13,13  ' RETURN CHARS  END'--------------.DECLAREØS$ØVAR'--------------  ' DECLARE VAR(S) IN S$  S$ = MID$(CURØSRCØLINE$, 10 - DEFINEØFLAG)  IGNOREØBRACKETS = 1  GOSUB PARSEØARGUMENTS  IGNOREØBRACKETS = 0  ' SPLIT PARAMETERS  NEXTØLINE$ = ""  ' NEW LINE IF DIMENSIONING...  IF ARGØCNT < 0 THEN BEGIN    PARSERØERROR$ = "?DECLARE PARAMETER MISSING IN LINE " + STR$(SRCØLINENO)    GOTO RETURNØTOØEDITORØWITHØERROR  BEND  FOR I = 0 TO ARGØCNT    VARØNAME$ = ARGS$(I)    GOSUB PARSEØDECLAREDØVAR  NEXT I  IF NEXTØLINE$ <> "" THEN BEGIN    DELETEØLINEØFLAG = 0    CURØSRCØLINE$ = "^^" + NEXTØLINE$  BEND : ELSE BEGIN    DELETEØLINEØFLAG = 1  BEND  RETURN'------------------.PARSEØDECLAREDØVAR'------------------  DIMENSION$ = ""  VALUE$ = ""  BKTØOPENØIDX = INSTR(VARØNAME$, "(")  BKTØCLOSEØIDX = INSTR(VARØNAME$, ")")  EQUALSØIDX = INSTR(VARØNAME$, "=")  IF EQUALSØIDX <> 0 THEN BEGIN  ' --- ASSIGNMENT    VALUE$ = MID$(VARØNAME$, EQUALSØIDX + 1)    VARØNAME$ = LEFT$(VARØNAME$, EQUALSØIDX - 1)    TR$ = WHITESPACE$    S$ = VARØNAME$    GOSUB STRIPØTR$ØFROMØEND    VARØNAME$ = S$    S$ = VALUE$    GOSUB STRIPØTR$ØFROMØBEGINNING    GOSUB STRIPØTR$ØFROMØEND    VALUE$ = S$    IF LEFT$(VALUE$, 1) = "$" THEN BEGIN      HX$ = MID$(VALUE$, 2)      GOSUB CHECKØHEX    BEND    IF LEFT$(VALUE$, 1) = "%" THEN BEGIN      BI$ = MID$(VALUE$, 2)      GOSUB CHECKØBINARY    BEND  BEND  IF BKTØOPENØIDX <> 0 AND BKTØCLOSEØIDX <> 0 THEN BEGIN  ' --- DIMENSION    DIMENSION$ = MID$(VARØNAME$, BKTØOPENØIDX + 1, BKTØCLOSEØIDX - BKTØOPENØIDX - 1)    CLEANØVARNAME$ = LEFT$(VARØNAME$, BKTØOPENØIDX - 1)    S$ = DIMENSION$    DONTØMARKØLABEL = 1    GOSUB REPLACEØVARSØANDØLABELS    DONTØMARKØLABEL = 0    DIMENSION$ = S$    VARØNAME$ = CLEANØVARNAME$  ' CHECK FOR DEFINE TOKENS    DELETEØLINEØFLAG = 0  BEND  TY = ‘Ÿ–Ø“≈¡Ã  ' VAR TYPE  T$ = RIGHT$(VARØNAME$, 1)  ' TYPE (IF ANY) IN T$  IF VERBOSE THEN BEGIN    PRINT "ADDING ";  BEND  IF INSTR("%&$", T$) = 0 THEN BEGIN    T$ = ""    TY = ‘Ÿ–Ø“≈¡Ã  BEND  IF DEFINEØFLAG = 1 THEN BEGIN    TY = ‘Ÿ–Øƒ≈∆  BEND  IF T$ = "%" THEN BEGIN    TY = ‘Ÿ–Ø…Œ‘  BEND  IF T$ = "$" THEN BEGIN    TY = ‘Ÿ–Ø”‘“  BEND  IF T$ = "&" THEN BEGIN    TY = ‘Ÿ–Ø¬Ÿ‘≈  BEND  VARØTABLE$(TY, ELEMENTØCNT(TY)) = VARØNAME$  IF DIMENSION$ <> "" THEN BEGIN    ID = ELEMENTØCNT(TY)    GOSUB GENERATEØVARNAME  ' FETCH VARNAME IN GENØVARNAME$    IF DEFINEØFLAG = 0 THEN BEGIN      NEXTØLINE$ = NEXTØLINE$ + "DIM " + GENØVARNAME$ + T$ + "(" + DIMENSION$ + "):"    BEND  BEND  IF VALUE$ <> "" THEN BEGIN    ID = ELEMENTØCNT(TY)    GOSUB GENERATEØVARNAME     IF DEFINEØFLAG=0 THEN BEGIN      NEXTØLINE$ = NEXTØLINE$ + GENØVARNAME$ + T$ + "=" + VALUE$ + ":"    BEND  BEND  IF DEFINEØFLAG = 1 THEN BEGIN    DEFINEØVAL$(ELEMENTØCNT(TY)) = VALUE$  BEND  IF VERBOSE THEN BEGIN    PRINT VARØNAME$; "í: "; ELEMENTØCNT(TY)  BEND  ELEMENTØCNT(TY) = ELEMENTØCNT(TY) + 1  RETURN'---------------.SETØOUTPUTØFILE'---------------  S$ = MID$(CURØSRCØLINE$, 8)  GOSUB PARSEØARGUMENTS  IF ARGØCNT <> 0 THEN BEGIN    PRINT "?INVALID PARAMETERS IN LINE ";SRCØLINENO    END  BEND  S$ = ARGS$(0)  TR$ = DBLØQUOTE$  GOSUB STRIPØTR$ØFROMØBEGINNING  GOSUB STRIPØTR$ØFROMØEND  ' QUOTES LEFT & RIGHT  IF VERBOSE THEN BEGIN    PRINT "SETTING OUTPUT FILE TO " + S$ + "í"  BEND  OF$ = S$  DELETEØLINEØFLAG = 1  ' DISABLE PASSTHROUGH  RETURN'------------------.ADDØTOØLABELØTABLE'------------------  IF VERBOSE THEN BEGIN    PRINT "LABEL "; CURØSRCØLINE$; " AT LINE "; DESTØLINENO  BEND  DELETEØLINEØFLAG = 1  LABELØNAME$(LABELØCNT) = MID$(CURØSRCØLINE$, 2)  LABELØLINENO(LABELØCNT) = DESTØLINENO + 1  LABELØCNT = LABELØCNT + 1  ' INCREASE LABEL COUNT  RETURN'---------------------------.RETURNØTOØEDITORØWITHØERROR'---------------------------  BANK 4  ' SET ERROR MAILBOX FLAG  FOR R=1 TO LEN(PARSERØERROR$)    POKE $4FF30 + R - 1, ASC(MID$(PARSERØERROR$, R, 1))  NEXT R  POKE $4FF30 + R - 1, 0  POKE $FF09, MOD(SRCØLINENO, 256)  POKE $FF0A, SRCØLINENO / 256  POKE $FF07, PEEK($FF07) OR 2  ' SET AUTOJUMP FLAG  DCLOSE  GOTO CHAINØEDITOR'------------------------.STRIPØTR$ØFROMØBEGINNING'------------------------  ' -- STRIP TR$ FROM BEGINNING OF STRING IN S$ --  DO WHILE INSTR(TR$, (LEFT$(S$, 1)))    S$ = MID$(S$, 2)  LOOP  RETURN'------------------.STRIPØTR$ØFROMØEND'------------------  ' -- STRIP CHARACTERS IN TR$ FROM END OF S$ --  DO WHILE INSTR(TR$, RIGHT$(S$, 1))    S$ = LEFT$(S$, LEN(S$) - 1)  LOOP  RETURN'---------------.PARSEØARGUMENTS'---------------  ' -- PARSE ARGUMENTS --  '     IN: S$ = STRING  '    OUT: ARGS$(X) = ARGUMENT LIST, ARGØCNT = ARGUMENT COUNT  '         ARGS$(0) = FIRST ARG, ARGS$(1) = SECOND ARG...    DELIM$ = ",;"  ARGØCNT = 0  ARGSØLIST$ = S$  ARGSØLISTØLEN = LEN(S$)  IGNOREØDELIMØFLAG = 0  IF ARGSØLISTØLEN = 0 THEN BEGIN    ARGØCNT = -1    RETURN  ' NO STRING  BEND  FOR ARGSØIDX = 0 TO 31    ARGS$(ARGSØIDX) = ""  NEXT ARGSØIDX  FOR ARGSØIDX = 1 TO ARGSØLISTØLEN    CURØCH$ = MID$(ARGSØLIST$, ARGSØIDX, 1)    IF CURØCH$ = "(" AND IGNOREØBRACKETS = 1 THEN IGNOREØDELIMØFLAG = 1    IF CURØCH$ = ")" AND IGNOREØBRACKETS = 1 THEN IGNOREØDELIMØFLAG = 0    IF INSTR(DELIM$, CURØCH$) = 0 OR IGNOREØDELIMØFLAG = 1 THEN BEGIN      ARGS$(ARGØCNT) = ARGS$(ARGØCNT) + CURØCH$    BEND : ELSE BEGIN      S$ = ARGS$(ARGØCNT)      TR$ = " "      GOSUB STRIPØTR$ØFROMØBEGINNING      GOSUB STRIPØTR$ØFROMØEND      ARGS$(ARGØCNT) = S$      ARGØCNT = ARGØCNT + 1    BEND  NEXT ARGSØIDX  S$ = ARGS$(ARGØCNT)  TR$=" "  GOSUB STRIPØTR$ØFROMØBEGINNING  GOSUB STRIPØTR$ØFROMØEND  ARGS$(ARGØCNT) = S$  S$ = ARGSØLIST$  ' RESTORE S$  RETURN'-----------------------.REPLACEØVARSØANDØLABELS'-----------------------  ' -- REPLACE VARS & LABELS IN SOURCE STRING --  '    IN:   S$ = SOURCE STRING  '    OUT:  S$ = DEST STRING WITH REPLACED ITEMS    IF LEFT$(S$, 2) = "^^" THEN BEGIN    S$ = RIGHT$(S$,LEN(S$)-2)    RETURN  BEND  QUOTEØFLAG = 0  A$ = ""  CURØTOK$ = ""  SHITTYØSYNTAXØFLAG = 0  EXPECTINGØLABEL = 0  DEFAULTØDELIM$ = "?<>=+-#*/^,.:;() "  DELIM$ = DEFAULTØDELIM$  FOR RVØIDX = 1 TO LEN(S$)    CURØCH$ = MID$(S$, RVØIDX, 1)    IF CURØCH$ = ":" AND QUOTEØFLAG = 0 THEN BEGIN      SHITTYØSYNTAXØFLAG = 0    BEND    IF SHITTYØSYNTAXØFLAG AND CURØCH$ = "(" THEN BEGIN      DELIM$ = DEFAULTØDELIM$    BEND    IF SHITTYØSYNTAXØFLAG AND CURØCH$ = ")" THEN BEGIN      DELIM$ = DEFAULTØDELIM$ + "DPUB"    BEND    IF CURØCH$ = DBLØQUOTE$ THEN BEGIN      QUOTEØFLAG = ABS(QUOTEØFLAG - 1)      IF QUOTEØFLAG = 1 THEN BEGIN        GOSUB CHECKØTOKENØFORØSUBBING        A$ = A$ + CURØTOK$        CURØTOK$ = ""      BEND : ELSE BEGIN        A$ = A$ + CURØCH$        CURØCH$ = ""      BEND    BEND    IF QUOTEØFLAG = 1 THEN BEGIN      A$ = A$ + CURØCH$      GOTO RVALØSKIP    BEND    IF INSTR(DELIM$, CURØCH$) <> 0 THEN BEGIN      GOSUB CHECKØTOKENØFORØSUBBING      A$ = A$ + CURØTOK$      CURØTOK$ = ""      IF CURØCH$ = " " THEN CURØCH$ = ""      A$ = A$ + CURØCH$    BEND : ELSE BEGIN      CURØTOK$ = CURØTOK$ + CURØCH$    BEND.RVALØSKIP  NEXT  GOSUB CHECKØTOKENØFORØSUBBING   S$ = A$ + CURØTOK$  RETURN  END'-----------------------.CHECKØTOKENØFORØSUBBING'-----------------------  IF CURØTOK$ = "" OR CURØTOK$ = "_" THEN RETURN  ' DECIMAL NUMBER CHECK  ' - - - - - - - - - -  IF VAL(CURØTOK$) <> 0 THEN BEGIN    EXPECTINGØLABEL = 0    RETURN  ' NEVER CHANGE NUMBERS  BEND  IF CURØTOK$ = "0" THEN BEGIN    CURØTOK$="."    RETURN  ' STUPID MS BASIC OPTIMIZATION  BEND  ' CHECK IF WE SHOULD MARK THIS EXPECTED LABEL  ' - - - - - - - - - - - - - - - - - - - - - -  IF EXPECTINGØLABEL AND DONTØMARKØLABEL = 0 THEN BEGIN    GOSUB MARKØCURØTOK$ØLABEL    EXPECTINGØLABEL = 0    RETURN  ' REPLACE LABEL  BEND  IF CURØTOK$ = "GOTO" THEN NEXTØLINEØFLAG = 1  ' ARE WE EXPECTING A LABEL NEXT?  ' - - - - - - - - - - - - - - -  IF CURØTOK$ = "GOTO" OR CURØTOK$ = "GOSUB" OR CURØTOK$ = "TRAP" THEN BEGIN    EXPECTINGØLABEL = 1  BEND  ' CHECK HEX VALUE  ' - - - - - - - -  IF LEFT$(CURØTOK$, 1) = "$" THEN BEGIN    HX$ = MID$(CURØTOK$, 2)    GOSUB CHECKØHEX    RETURN   BEND  ' CHECK BINARY VALUE  ' - - - - - - - - -  IF LEFT$(CURØTOK$, 1) = "%" THEN BEGIN    BI$ = MID$(CURØTOK$, 2)    GOSUB CHECKØBINARY    RETURN  BEND  TOKØNAME$ = " " + CURØTOK$ + " "  ' IGNORE EXISTING VOCABULARY OF FUNCTIONS/COMMANDS  ' - - - - - - - - - - - - - - - - - - - - - - - -   FOR T = 0 TO 6    IF INSTR(TOKENS$(T), TOKØNAME$) <> 0 THEN BEGIN      LC$ = CURØTOK$      GOSUB CHECKØIFØCOMMANDØTRIGGERSØSHITTYØSYNTAX      RETURN    BEND  NEXT  ' CHECK TO SWAP OUT VARIABLES WITH SHORT NAMES  ' - - - - - - - - - - - - - - - - - - - - - -  DIDØREPLACEØFLAG = 0  ' DID REPLACE FLAG  T$ = RIGHT$(CURØTOK$, 1)  TY = ‘Ÿ–Ø“≈¡Ã  IF T$ = "%" THEN TY = ‘Ÿ–Ø…Œ‘  IF T$ = "$" THEN TY = ‘Ÿ–Ø”‘“  IF T$ = "&" THEN TY = ‘Ÿ–Ø¬Ÿ‘≈  FOR ID = 0 TO ELEMENTØCNT(TY)    IF CURØTOK$ = VARØTABLE$(TY, ID) THEN BEGIN      GOSUB GENERATEØVARNAME      CURØTOK$ = GENØVARNAME$ + TYPEØIDENT$(TY)      ID = ELEMENTØCNT(TY)      DIDØREPLACEØFLAG = 1    BEND  NEXT ID  IF DIDØREPLACEØFLAG = 1 THEN RETURN  ' CHECK DEFINES TABLE TOO  ' - - - - - - - - - - - -  FOR ID = 0 TO ELEMENTØCNT(‘Ÿ–Øƒ≈∆)    IF CURØTOK$ = VARØTABLE$(‘Ÿ–Øƒ≈∆, ID) THEN BEGIN      CURØTOK$ = DEFINEØVAL$(ID)      RETURN    BEND  NEXT ID  ' CHECK FOR STRUCT NAMES  ' ----------------------  FOUNDØSTRUCTØIDX = -1  FOR ID = 0 TO STRUCTØCNT - 1  ' CHECK STRUCT NAMES    IF CURØTOK$ = STRUCTØNAME$(ID) THEN BEGIN      GOSUB CHECKØFORØCREATIONØOFØSTRUCTØOBJECT      FOUNDØSTRUCTØIDX = ID      ID = STRUCTØCNT - 1  ' CREATE NEW STRUCT OBJECT    BEND  NEXT ID  IF FOUNDØSTRUCTØIDX <> -1 THEN BEGIN    RETURN  BEND  ' –… TOKEN CHECK  ' - - - - - - -  IF ASC(CURØTOK$) = 222 THEN RETURN  ' CHECK FOR DUMB COMMANDS  ' - - - - - - - - - - - -  IF INSTR(DUMBØCMDS$, LC$) <> 0 AND _    (CURØTOK$ = "R" OR CURØTOK$ = "P" _     OR CURØTOK$ = "U8" OR CURØTOK$ = "W"     OR CURØTOK$ = "L" OR CURØTOK$ = "U") THEN BEGIN    RETURN  BEND.UNRESOLVEDØCURØTOK$  PARSERØERROR$ = "?UNRESOLVED IDENTIFIER: '" + CURØTOK$ + "' IN LINE " + STR$(SRCØLINENO)  SLEEP 1  GOTO RETURNØTOØEDITORØWITHØERROR  RETURN'---------------------------------------.CHECKØIFØCOMMANDØTRIGGERSØSHITTYØSYNTAX'---------------------------------------  ' CHECK IF COMMAND TRIGGERS SHITTY SYNTAX MODE  ' TODO: COMPARE UBIK'S LOGIC HERE VERSUS MY OWN 4079 LC$="DOPEN" LOGIC  RETURN'-------------.MARKØCURØTOK$ØLABEL'-------------  CURØTOK$ = MK$ + CURØTOK$ + MK$  RETURN'-------------------------------.SWAPØOUTØLABELØINØCURØTOK$ØFORØLINENO'-------------------------------  DIDØREPLACEØFLAG = 0  FOR ID = 0 TO LABELØCNT - 1    IF CURØTOK$ = LABELØNAME$(ID) THEN BEGIN      CURØTOK$ = STR$(LABELØLINENO(ID))      ID = LABELØCNT      DIDØREPLACEØFLAG = 1    BEND  NEXT ID  IF DIDØREPLACEØFLAG THEN RETURN  PARSERØERROR$ = "?UNRESOLVED LABEL: '" + CURØTOK$ + "' IN LINE" + STR$(MAPØDESTØTOØSRCØLINENO%(CURØDESTØLINENO - 1))  SLEEP 1  GOTO RETURNØTOØEDITORØWITHØERROR  RETURN'------------.CHECKØBINARY'------------  BR = 0  ' RESULT  FOR B = 0 TO LEN(BI$) - 1    BC$ = MID$(BI$, LEN(BI$) - B, 1)    IF BC$ <> "1" AND BC$ <> "0" THEN BEGIN      BANK 4      POKE $FF08, 132      GOTO UNRESOLVEDØCURØTOK$    BEND    IF BC$="1" THEN BR=BR+BINØCONV(B)  NEXT B  RETURN'---------.CHECKØHEX'---------  TRAP ILLEGALØHEXØHANDLER  VL = DEC(HX$)  TRAP  RETURN'-------------------.ILLEGALØHEXØHANDLER'-------------------  TRAP  BANK 4  POKE $FF08, 131  ' SET ILLEGAL HEX  GOTO UNRESOLVEDØCURØTOK$  ' JUMP INTO ERROR HANDLER'----------------.GENERATEØVARNAME'----------------  ' GENERATE VARNAME FROM INDEX  IF ID < 26 THEN BEGIN    GENØVARNAME$ = CHR$(65 + ID)    RETURN  BEND  N2 = MOD(ID, 26)  N1 = INT(ID / 26) - 1  GENØVARNAME$ = CHR$(65 + N1) + CHR$(65 + N2)  ' AVOID ANY BASIC TERMS AS VAR NAMES  IF GENØVARNAME$="DO" THEN GENØVARNAME$="D1"  IF GENØVARNAME$="GO" THEN GENØVARNAME$="G1"  IF GENØVARNAME$="TO" THEN GENØVARNAME$="T1"  IF GENØVARNAME$="DS" THEN GENØVARNAME$="D2"  IF GENØVARNAME$="DT" THEN GENØVARNAME$="D3"  IF GENØVARNAME$="EL" THEN GENØVARNAME$="E1"  IF GENØVARNAME$="ER" THEN GENØVARNAME$="E2"  IF GENØVARNAME$="FN" THEN GENØVARNAME$="F1"  IF GENØVARNAME$="IF" THEN GENØVARNAME$="I1"  RETURN  STOP'------------.GETØFILENAME'------------  BANK 4  BA = $FF00  IF PEEK(BA+0) = ASC("S") AND PEEK(BA+1) = ASC("K") THEN BEGIN    VERBOSE = PEEK($FF07) AND 8    F$ = ""    A = BA + $10    DO WHILE PEEK(A) <> 0      F$ = F$ + CHR$(PEEK(A))      A = A + 1    LOOP    IF PEEK($FF07) AND 1 THEN RETURN    PRINT "FILENAME? " + F$    PRINT "ë";  BEND  INPUT "FILENAME"; A$  IF A$ = "" THEN BEGIN    PRINT "NO FILENAME SET"    END  BEND  POKE BA, ASC("S")  POKE BA + 1, ASC("K")  FOR R = 1 TO 16    POKE BA + 8 + R - 1, ASC(MID$(A$, R, 1))  NEXT R  F$ = A$  RETURN'-------------.SAVEØFILENAME'-------------  AD = $FF30  BANK 4  FOR R = 0 TO 16    POKE AD + R, 0  NEXT R  IF OF$ <> "" THEN BEGIN    TF$ = OF$  BEND : ELSE BEGIN    TF$="ELEVEN.OUT"  BEND  FOR R = 1 TO LEN(TF$)    POKE AD + R - 1, ASC(MID$(TF$, R, 1))  NEXT R  RETURN'------------.CHAINØEDITOR'------------  GET A$  IF A$ <> "" THEN BEGIN    INPUT ZZ    IF ZZ=1 THEN STOP  BEND  PRINT "ìEDMA 0,$D400,$8000000,$2001:NEW RESTORE"  PRINT "RUN";  ' LOAD '11.EDIT' FROM CACHE  BANK 128  POKE 208, 2      ' NO# OF CHARS IN KEYBOARD BUFFER  POKE 688, 13, 13 ' RETURN CHARS  END'-------------.ISØS$ØDEFINED'-------------  INSIDEØIFDEF = 1  FOR K = 0 TO ELEMENTØCNT(‘Ÿ–Øƒ≈∆)    IF VARØTABLE$(‘Ÿ–Øƒ≈∆, K) = S$ THEN BEGIN      INSIDEØIFDEF = 0      RETURN    BEND  NEXT K  RETURN'----------------------.READØINØSTRUCTØDETAILS'----------------------  CURØSRCØLINE$ = MID$(CURØSRCØLINE$, 9)  GOSUB READØNEXTØTOKEN  ' GET NEXT TOKEN IN S$  IF S$ = "" THEN BEGIN    PRINT "ERROR: NO STRUCT NAME FOUND"    SLEEP 1    RETURN  BEND  STRUCTØNAME$(STRUCTØCNT) = S$  STRUCTØVARS$(STRUCTØCNT) = CURØSRCØLINE$  STRUCTØCNT = STRUCTØCNT + 1  RETURN'---------------.READØNEXTØTOKEN'---------------  ' READ NEXT TOKEN FROM CURØSRCØLINE$ INTO S$  S$ = CURØSRCØLINE$  GOSUB STRIPØTR$ØFROMØBEGINNING  GOSUB STRIPØTR$ØFROMØEND  CURØSRCØLINE$ = S$  SF$ = " "  SF = 0  IF LEFT$(S$, 1) = DBLØQUOTE$ THEN BEGIN    SF$ = DBLØQUOTE$ + ", "    SF = 2  BEND  A = INSTR(CURØSRCØLINE$, SF$)  IF A <> 0 THEN BEGIN    S$ = MID$(CURØSRCØLINE$, 1, INSTR(CURØSRCØLINE$, SF$) + SF - 1)    CURØSRCØLINE$ = MID$(CURØSRCØLINE$, INSTR(CURØSRCØLINE$, SF$) + SF + 1)  BEND  IF A = 0 THEN BEGIN    S$ = CURØSRCØLINE$    CURØSRCØLINE$ = ""  BEND  RETURN'--------------.READØNEXTØLINE'--------------  ' READ NEXT LINE INTO CURØSRCØLINE$  BANK 0  CURØLINEØLEN = PEEK(CURØATTICØADDR)  CURØSRCØLINE$ = LEFT$(BLANKØLINE$, CURØLINEØLEN)  SRCØLINEØPTR = POINTER(CURØSRCØLINE$)  SRCØLINEBUFFØPTR = $10000 + WPEEK(SRCØLINEØPTR + 1)  CURØATTICØADDR = CURØATTICØADDR + 1  IF CURØLINEØLEN <> 0 THEN BEGIN    EDMA 0, CURØLINEØLEN, CURØATTICØADDR, SRCØLINEBUFFØPTR    CURØATTICØADDR = CURØATTICØADDR + CURØLINEØLEN  BEND  CURØSRCØLINENO = CURØSRCØLINENO + 1  TR$ = WHITESPACE$  S$ = CURØSRCØLINE$  GOSUB STRIPØTR$ØFROMØBEGINNING  CURØSRCØLINE$ = S$  QUOTEØFLAG = 0    ' QUOTES ON  CUTØTAILØIDX = 0  ' CUT CHARS FROM TAIL  BANK 1  RETURN'-------------------------.SINGLEØQUOTEØCOMMENTØTRIM'-------------------------  CUTØTAILØIDX = INSTR(CURØSRCØLINE$, "'")  ' SINGLE QUOTE  IF CUTØTAILØIDX <> 0 THEN BEGIN    IF INSTR(CURØSRCØLINE$, DBLØQUOTE$) <> 0 THEN BEGIN  ' DOUBLE QUOTE      CUTØTAILØIDX = 0      BANK 0      CURØLINEØLENØMINUSØONE = PEEK(SRCØLINEØPTR) - 1      CURØLINEBUFFØADDR = WPEEK(SRCØLINEØPTR + 1)      BANK 1      FOR R = 0 TO CURØLINEØLENØMINUSØONE        CHR = PEEK(CURØLINEBUFFØADDR + R)        IF CHR = DBLØQUOTEØCHAR THEN BEGIN          QUOTEØFLAG = ABS(QUOTEØFLAG - 1)        BEND : ELSE BEGIN          IF CHR = SNGLØQUOTEØCHAR AND QUOTEØFLAG=0 THEN BEGIN            CUTØTAILØIDX = R + 1            R = 999          BEND        BEND      NEXT    BEND    IF CUTØTAILØIDX THEN BEGIN      CURØSRCØLINE$ = LEFT$(CURØSRCØLINE$, CUTØTAILØIDX - 1)    BEND  BEND  RETURN'----------------------------.PARSEØPREPROCESSORØDIRECTIVE'----------------------------  IF INSTR(CURØSRCØLINE$, "IFDEF") = 2 THEN BEGIN    S$ = MID$(CURØSRCØLINE$, 8)    GOSUB ISØS$ØDEFINED    DELETEØLINEØFLAG = 1  BEND  IF INSTR(CURØSRCØLINE$, "ENDIF") = 2 THEN BEGIN    INSIDEØIFDEF = 0    DELETEØLINEØFLAG = 1  BEND  IF INSTR(CURØSRCØLINE$, "DEFINE") = 2 THEN BEGIN    DEFINEØFLAG = 1    GOSUB DECLAREØS$ØVAR    DEFINEØFLAG = 0  BEND  IF INSTR(CURØSRCØLINE$, "DECLARE") = 2 THEN BEGIN    DEFINEØFLAG = 0    GOSUB DECLAREØS$ØVAR   BEND  IF INSTR(CURØSRCØLINE$,"OUTPUT")=2 THEN BEGIN    GOSUB SETØOUTPUTØFILE  BEND  IF INSTR(CURØSRCØLINE$,"STRUCT")=2 THEN BEGIN    GOSUB READØINØSTRUCTØDETAILS    DELETEØLINEØFLAG = 1  BEND  RETURN'-----------------------------------.CHECKØFORØCREATIONØOFØSTRUCTØOBJECT'-----------------------------------  CO$ = S$  CURØSRCØLINE$ = S$  FOUNDØIDX = -1  ' PRESERVE ORIGINAL STRING  GOSUB READØNEXTØTOKEN  ' READ NEXT TOKEN FROM CURØSRCØLINE$ INTO S$  FOR RIDX = 0 TO STRUCTØCNT - 1    IF S$=STRUCTØNAME$(RIDX) THEN BEGIN      FOUNDØIDX = RIDX      RIDX = STRUCTØCNT - 1    BEND  NEXT RIDX  IF FOUNDØIDX = -1 THEN BEGIN    CURØSRCØLINE$ = CO$    S$ = CO$    RETURN  BEND  GOSUB READØNEXTØTOKEN  SK$ = S$  ' GET STRUCT OBJECT NAME  BL = INSTR(SK$, "(")  ' *** FOUND IT, SO MAKE DIM'S FOR EACH MEMBER VAR  S$ = STRUCTØVARS$(FOUNDØIDX)  GOSUB PARSEØARGUMENTS  ' PARSE ARGS INTO ARGS$(), ARGØCNT  FOUNDØIDX = 1  SZ = 0  FOR RIDX = 0 TO ARGØCNT    IF ARGS$(RIDX) <> "" THEN BEGIN      PRINT "ARGS$(";RIDX;")=";ARGS$(RIDX)      IF BL = 0 THEN BEGIN        S$ = SK$ + "Ø" + ARGS$(RIDX)        PRINT "STRUCT: "; S$        VARØNAME$ = S$        GOSUB PARSEØDECLAREDØVAR       BEND      IF BL <> 0 THEN BEGIN        S$ = LEFT$(SK$, BL - 1) + "Ø" + ARGS$(RIDX) + MID$(SK$, BL)        PRINT "STRUCT: ";S$        VARØNAME$ = S$        GOSUB PARSEØDECLAREDØVAR        STRUCTØFIELDS$(SZ) = VARØNAME$        SZ = SZ + 1      ' IF NEXTØLINE$<>"" THEN DELETEØLINEØFLAG=0:CURØSRCØLINE$="^^"+NEXTØLINE$:ELSE DELETEØLINEØFLAG=1      BEND    BEND  NEXT RIDX  S$ = NEXTØLINE$  GOSUB SAFEØADDØTOØCURRENTØORØNEXTØLINE  NEXTØLINE$ = ""  ' SAFE ADD CURØDESTØLINE$+S$ TO CURRENT DESTØLINE$(DESTØLINENO)  GOSUB READØNEXTØTOKEN  IF S$ <> "=" THEN BEGIN    CURØSRCØLINE$=""    RETURN  BEND  GOSUB READØNEXTØTOKEN  SZ=0  SR=0  SM=0  ' READ NEXT TOKEN FROM CURØSRCØLINE$ INTO S$  DO WHILE S$ <> ""    IF S$ = "_" THEN BEGIN      SRCØLINENO = SRCØLINENO + 1      GOSUB READØNEXTØLINE      GOTO CFCOSOØSKIP  ' READ NEXT LINE    BEND    IF SM = 0 AND S$ <> "[" THEN BEGIN      PRINT "ERROR: EXPECTED ["      SLEEP 1      STOP    BEND    IF SM = 0 AND S$ = "[" THEN BEGIN      SM = 1      GOTO CFCOSOØSKIP    BEND    IF SM = 1 AND S$ <> "[" AND S$ <> "]" THEN BEGIN      PRINT "ERROR: EXPECTED [ OR ]"      SLEEP 1      STOP    BEND    IF SM = 2 THEN BEGIN      IF LEFT$(S$, 1) = "]" THEN BEGIN        SR = SR + 1        SZ = 0        SM = 1        S$ = ""        GOTO CFCOSOØNEXTROW  ' NEXT ROW      BEND      ' IF LEFT$(S$, 1) = DBLØQUOTE$ THEN BEGIN      ' SS$ = S$      ' TR$ = ""      ' DO WHILE RIGHT$(SS$, 2) <> (DBLØQUOTE$ + ",")      '   GOSUB READØNEXTØTOKEN      '   SS$ = SS$ + S$      '   PRINT SS$      ' LOOP      ' S$ = SS$      ' TR$ = WHITESPACE$      ' STOP      IF RIGHT$(S$, 1) = "," THEN BEGIN        S$ = LEFT$(S$, LEN(S$) - 1)      BEND      S$ = STRUCTØFIELDS$(SZ) + "(" + STR$(SR) + ")=" + S$      GOSUB REPLACEØVARSØANDØLABELS      GOSUB SAFEØADDØTOØCURRENTØORØNEXTØLINE      S$ = ""      SZ = SZ + 1  ' SAFE ADD TO DESTØLINE$(DESTØLINENO).CFCOSOØNEXTROW    BEND    IF SM = 1 AND S$ = "[" THEN BEGIN      SM = 2      SZ = 0    BEND    IF SM = 1 AND S$ = "]" THEN BEGIN      SM = 0    BEND.CFCOSOØSKIP    GOSUB READØNEXTØTOKEN  ' READ NEXT TOKEN FROM CURØSRCØLINE$ INTO S$  LOOP  S$ = ""  CURØSRCØLINE$ = ""  NEXTØLINE$ = ""  ZZ$ = "Z"  RETURN'---------------------------------.CHECKØFORØCONTINUEØONTOØNEXTØLINE'---------------------------------  IF RIGHT$(CURØDESTØLINE$, 1) = "_" THEN BEGIN    CURØDESTØLINE$ = LEFT$(CURØDESTØLINE$, LEN(CURØDESTØLINE$) - 1)    NEXTØLINEØFLAG = 0    CONTØNEXTØLINEØFLAG = 1  BEND: ELSE BEGIN    CONTØNEXTØLINEØFLAG = 0  BEND  RETURN'--------------------------------.SAFEØADDØTOØCURRENTØORØNEXTØLINE'--------------------------------  ' --- SAFE ADD CURØDESTØLINE$+S$ TO CURRENT OR NEXT DESTØLINE$(DESTØLINENO)  IF LEN(CURØDESTØLINE$) + LEN(S$) + LEN(STR$(DESTØLINENO)) >= 159 THEN BEGIN    NEXTØLINEØFLAG=1  BEND  IF ZZ$ <> "" THEN BEGIN    S$ = ""    ZZ$ = ""    RETURN  ' FORCE S$ TO EMPTY  BEND  IF NEXTØLINEØFLAG = 1 THEN BEGIN    DESTØLINE$(DESTØLINENO) = CURØDESTØLINE$    CURØDESTØLINE$ = S$    MAPØDESTØTOØSRCØLINENO%(DESTØLINENO) = SRCØLINENO    DESTØLINENO = DESTØLINENO + 1    NEXTØLINEØFLAG = 0  BEND : ELSE BEGIN  ' -- ADD TO CURØDESTØLINE$    IF CURØDESTØLINE$ <> "" AND CONTØNEXTØLINEØFLAG = 0 AND RIGHT$(CURØDESTØLINE$,1) <> ":" THEN BEGIN      CURØDESTØLINE$ = CURØDESTØLINE$ + ":"    BEND        CURØDESTØLINE$ = CURØDESTØLINE$ + S$  BEND  IF VERBOSE THEN PRINT "<<" DESTØLINENO; S$  RETURN'---------.DUMPØVARS'---------  FOR TY = 0 TO 3    FOR ID = 0 TO ELEMENTØCNT(TY)      GOSUB GENERATEØVARNAME  ' GET GENØVARNAME$ (OPTIMISED VAR NAME)      PRINT#1, STR$(CURØDESTØLINENO) + " REM " _        + VARØTABLE$(TY,ID) + " = :" + GENØVARNAME$ + TYPEØIDENT$(TY) + ":"      CURØDESTØLINENO = CURØDESTØLINENO + 1    NEXT ID  NEXT TY  FOR ID = 0 TO LABELØCNT    PRINT#1, STR$(CURØDESTØLINENO) + " REM L" _      + MID$(STR$(LABELØLINENO(ID)), 2) + " = ." _      + LABELØNAME$(ID)      CURØDESTØLINENO = CURØDESTØLINENO + 1  NEXT ID  RETURN